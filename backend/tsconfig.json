{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@lib/*": ["lib/*"],
      "@routes/*": ["routes/*"]
    },
    "module": "CommonJS",
    "moduleResolution": "Node",
    "target": "ES2020",
    "types": ["node"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "include": ["src", "prisma","src/types"]
}